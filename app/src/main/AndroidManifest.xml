<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.msu.team23.project2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_spartycheckers"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_spartycheckers_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreateUserActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".GameOverActivity" />
        <activity android:name=".CheckersActivity" />
        <activity
            android:name=".LoginActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>